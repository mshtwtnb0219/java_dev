package collection;

import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		
		// ArrayListの宣言
		ArrayList<String> array = new ArrayList<>();
		array.add("test");
		array.add("test1");
		array.add("test2");
		
		for(int i=0; i < array.size(); i++) {
			System.out.println(array.get(i);
		}
	}

}
