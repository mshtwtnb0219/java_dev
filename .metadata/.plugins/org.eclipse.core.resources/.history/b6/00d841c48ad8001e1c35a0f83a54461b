package Scanner;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		// Scannerクラスのオブジェクトを生成する
		Scanner sc = new Scanner(System.in);
		// 空白文字まで入力した内容を取得する
		String str = sc.nextLine();
		// 入力内容を出力する
		System.out.println(str);
		// Scannerオブジェクトをクローズする
		sc.close();
		
		
		

	}

}
